        tools:layout_editor_absoluteX="-447dp"
        tools:layout_editor_absoluteY="-16dp" />

    <Button
        android:id="@+id/bt_return"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:background="@drawable/bt_return_updown"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.055"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.016" />

    <Button
        android:id="@+id/bt_save"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:background="@drawable/bt_save_updown"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.939"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.016" />

    <Button
        android:id="@+id/bt_set"
        android:layout_width="325dp"
        android:layout_height="160dp"
        android:background="@color/touMing"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <com.warkiz.widget.IndicatorStayLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.232">

        <com.warkiz.widget.IndicatorSeekBar
            android:id="@+id/sk_t1"
            android:layout_width="480dp"
            android:layout_height="51dp"
            android:maxHeight="26dp"
            android:minHeight="26dp"
            app:isb_indicator_color="#EA5A5A"
            app:isb_indicator_text_size="30dp"
            app:isb_max="30"
            app:isb_min="0"
            app:isb_progress="3"
            app:isb_seek_smoothly="true"
            app:isb_show_indicator="rounded_rectangle"
            app:isb_thumb_color="#EA5A5A"
            app:isb_thumb_size="20dp"
            app:isb_track_background_color="#44464F"
            app:isb_track_background_size="8dp"
            app:isb_track_progress_color="#EA5A5A"
            app:isb_track_progress_size="10dp"
            app:isb_track_rounded_corners="true"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/iv_background"
            app:layout_constraintHorizontal_bias="0.679"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.928" />
    </com.warkiz.widget.IndicatorStayLayout>

    <com.warkiz.widget.IndicatorStayLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.35">

        <com.warkiz.widget.IndicatorSeekBar
